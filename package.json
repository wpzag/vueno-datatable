{
  "name": "vueno-datatable",
  "private": false,
  "version": "0.0.3",
  "type": "module",
  "module": "./dist/vueno-datatable.js",
  "main": "./dist/vueno-datatable.umd.cjs",
  "exports": {
    ".": {
      "import": "./dist/vueno-datatable.js",
      "require": "./dist/vueno-datatable.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "types": "./dist/types/main.d.ts",
  "files": [
    "dist",
    "web-types.json",
    "package.json"
  ],
  "web-types": "./web-types.json",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build-watch": "vue-tsc && vite build --watch",
    "preview": "vite preview",
    "docs:build": "pnpm  build && pnpm --filter @vueno/docs build",
    "release": "bumpp package.json"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "bumpp": "^8.2.1",
    "@iconify/json": "^2.1.156",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vueuse/core": "^9.8.2",
    "sass": "^1.57.1",
    "typescript": "^4.9.3",
    "unocss": "^0.48.0",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1",
    "vue-tsc": "^1.0.11"
  }
}
